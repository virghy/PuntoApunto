	PROCEDURE genera_formapago		&& Metodo que genera los item de forma de pagos
		Local LCTIPO, LNMONTO, LNIDPAGO, LCMONEDA, LNCOTIZA, IMP_PAG
		LCTIPO = '1'
		LNIDPAGO = VT_VPAGOS.IDPAGO
