	PROCEDURE Pageframe1.PAGE1.totales.Refresh
		
		Local LNTOTAL, LNDESCUENTO, LNPORCDESCUENTO
		This.IVA.EDITABLE = VT_VFACTURA.TOTALMANUAL
		this.exento.editable=VT_VFACTURA.TOTALMANUAL
		this.gravado.editable=VT_VFACTURA.TOTALMANUAL
		
		
		If Thisform.EDITMODE
			LNDESCUENTO = NVL(This.DESCUENTO.Value,0)
			Select VT_VDETFACTU
		
			This.Parent.TSGRID1.SUMCOLUMN()
			LNPORCDESCUENTO = NVL(This.PORCENTAJE.Value,0)
			lnDescuento = NVL(This.DESCUENTO.Value,0)
		
			NRODECIMALES = CU_MONEDA.DECIMALES
		
		*VG 15/06/2011
		*Calculamos el descuento de acuerdo al tipo: En porcentaje o el Valor	
			m.FormaDesc=This.cboFormaDesc.Value
			IF m.FormaDesc="P"
				If LNPORCDESCUENTO>0
					This.DESCUENTO.Value = ROUND(This.Parent.TSGRID1.TOTALES(1)*LNPORCDESCUENTO /100,nrodecimales)
				ENDIF
				
			Else
				If This.DESCUENTO.Value>0
					LNPORCDESCUENTO = ROUND((lnDescuento *100/(This.Parent.TSGRID1.TOTALES(1))),6)
				Else
					LNPORCDESCUENTO = 0
				ENDIF
				
				This.PORCENTAJE.Value = LNPORCDESCUENTO
				
			ENDIF
			*SET STEP ON
			DEBUGOUT LNPORCDESCUENTO
			If This.Parent.TSGRID1.TOTALES(2)<>0
				This.EXENTO.Value = ROUND(This.Parent.TSGRID1.TOTALES(2)-(This.Parent.TSGRID1.TOTALES(2)*LNPORCDESCUENTO/100),nrodecimales)
			Else
				This.EXENTO.Value = 0
			ENDIF
			
			If  .Not. VT_VFACTURA.TOTALMANUAL
				If XVENTA.TIPO_IVA<>'C'
					If This.Parent.TSGRID1.TOTALES(3)<>0 OR This.Parent.TSGRID1.TOTALES(4)<>0 
						m.Gravada5= ROUND(This.Parent.TSGRID1.TOTALES(3)-This.Parent.TSGRID1.TOTALES(3)*LNPORCDESCUENTO/100,nrodecimales) 
						m.Gravada10=ROUND(This.Parent.TSGRID1.TOTALES(4)-This.Parent.TSGRID1.TOTALES(4)*LNPORCDESCUENTO/100,nrodecimales) 
						
						Replace Gravada5 WITH ROUND(m.Gravada5,nrodecimales) IN vt_vFactura
						Replace Gravada10 WITH ROUND(m.Gravada10,nrodecimales) IN vt_vFactura
						
						Replace Iva5 WITH ROUND(m.Gravada5*1.05,nrodecimales) IN vt_vFactura
						Replace Iva10 WITH ROUND(m.Gravada10*1.1,nrodecimales) IN vt_vFactura
						
						
						This.IVA.Value = NVL(vt_vFactura.Iva5,0) + NVL(vt_vFactura.Iva10,0) 
						This.GRAVADO.Value=  NVL(vt_vFactura.Gravada5,0) + NVL(vt_vFactura.Gravada10,0) 
						
					Else
						This.IVA.Value = 0
						This.GRAVADO.Value=0
					ENDIF
					
		*!*				If This.Parent.TSGRID1.TOTALES(2)<>0
		*!*					This.GRAVADO.Value = ROUND(This.Parent.TSGRID1.TOTALES(2)-(This.Parent.TSGRID1.TOTALES(2)*LNPORCDESCUENTO/100),nrodecimales)
		*!*				Else
		*!*					This.GRAVADO.Value = 0
		*!*				ENDIF
					
				ELSE
				
						m.Gravada5= ROUND(This.Parent.TSGRID1.TOTALES(3)-This.Parent.TSGRID1.TOTALES(3)*LNPORCDESCUENTO/100,nrodecimales) 
						m.Gravada10=ROUND(This.Parent.TSGRID1.TOTALES(4)-This.Parent.TSGRID1.TOTALES(4)*LNPORCDESCUENTO/100,nrodecimales) 
						
		*				IF vt_vFactura.Gravada5<>ROUND(m.Gravada5/1.05,nrodecimales)
							Replace Gravada5 WITH ROUND(m.Gravada5/1.05,nrodecimales) IN vt_vFactura
		*				ENDIF
						
		*				IF 	vt_vFactura.Gravada10 <>ROUND(m.Gravada10/1.1,nrodecimales)
							Replace Gravada10 WITH ROUND(m.Gravada10/1.1,nrodecimales) IN vt_vFactura
		*				ENDIF
							
		*				IF vt_vFactura.Iva5<>ROUND(m.Gravada5/21,nrodecimales)
							Replace Iva5 WITH ROUND(m.Gravada5/21,nrodecimales) IN vt_vFactura
		*				ENDIF
						
		*				IF 	vt_vFactura.Iva10<>ROUND(m.Gravada10/11,nrodecimales)
							Replace Iva10 WITH ROUND(m.Gravada10/11,nrodecimales) IN vt_vFactura
		*				ENDIF
						
						
		*				IF This.IVA.Value<> NVL(vt_vFactura.Iva5,0) + NVL(vt_vFactura.Iva10,0) 
							This.IVA.Value = NVL(vt_vFactura.Iva5,0) + NVL(vt_vFactura.Iva10,0) 
		*				ENDIF
						
		*				IF 	This.GRAVADO.Value<>NVL(vt_vFactura.Gravada5,0) + NVL(vt_vFactura.Gravada10,0) 
							This.GRAVADO.Value=  NVL(vt_vFactura.Gravada5,0) + NVL(vt_vFactura.Gravada10,0) 
		*				ENDIF
							
						
		*!*				m.Iva = This.Parent.TSGRID1.TOTALES(3)
		*!*				This.IVA.Value = ROUND(M.Iva-(M.Iva*LNPORCDESCUENTO/100),nrodecimales)
		*!*				This.GRAVADO.Value = round(This.Parent.TSGRID1.TOTALES(2)-(This.Parent.TSGRID1.TOTALES(2)*LNPORCDESCUENTO/100),nrodecimales)-This.IVA.Value
				ENDIF
			ENDIF
			
		ENDIF
		
		LCALIAS = Alias()
		Select XVENTA
		Locate For IDCOMPROBANTE=VT_VFACTURA.IDCOMPROBANTE
		Select (LCALIAS)
		This.SUBTOTAL.Value = VT_VFACTURA.EXENTA+VT_VFACTURA.GRAVADA
		This.Total.Value = VT_VFACTURA.EXENTA+VT_VFACTURA.GRAVADA+VT_VFACTURA.IVA
		
		
		
	ENDPROC

