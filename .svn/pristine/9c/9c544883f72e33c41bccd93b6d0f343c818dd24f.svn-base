	PROCEDURE Init
		LPARAMETERS IdDespacho
		DODEFAULT()
		IF PCOUNT()=1
			this.refreshform(IdDespacho)
			this.editar()
			ThisForm.Pageframe1.ActivePage=3
		ENDIF
		
		
		*!*	LPARAMETERS NroTelefono
		*!*	DODEFAULT()
		*!*	IF PCOUNT()=1
		*!*		this.addnew()
		*!*		ThisForm.Pageframe1.Page1.txtTelefono.Value=NroTelefono
		*!*	ENDIF
		DIMENSION PERMISO(6)
		PERMISO = OAPP.PERMISOS(200)
		THISFORM.PAGEFRAME1.PAGE4.TSGRID1.PRECIO.READONLY =  .NOT. PERMISO(1)
		
		*ThisForm.Pageframe1.Page3.Enabled=OAPP.PERMISOS(466)
			
	ENDPROC

	PROCEDURE Pageframe1.Page3.Activate
		this.Refresh()
	ENDPROC

	PROCEDURE Pageframe1.Page4.Activate
		this.Refresh()
	ENDPROC

	PROCEDURE Pageframe1.Page5.Activate
		this.Refresh()
	ENDPROC

	PROCEDURE Refresh
		IF ! thisform.editmode
			ThisForm.Pageframe1.page3.Refresh()
			ThisForm.Pageframe1.page4.Refresh()
			ThisForm.Pageframe1.page5.Refresh()
		ENDIF
			
			
	ENDPROC

	PROCEDURE validar
		
		*!*	*m.idseguro = thisFORM.pageframe1.page1.tscombobox3.Value
		*!*	*m.idtiposervicio = thisform.pageframe1.page1.tscombobox1.Value
		*!*	m.idseguro = sas_solicitud.idseguro
		*!*	m.idtiposervicio = sas_solicitud.idtiposervicio
		*SET STEP ON 
		
		
		*Generamos el detalle 
		SELECT os_movimiento_detalle
		*SCAN FOR talle='xx'
		
		IF EMPTY(NVL(sas_solicitud.idproductoFinal,''))
			m.IdProducto=sas_solicitud.idproducto
			m.Descripcion=ThisForm.Pageframe1.Page1.Producto.Value
		ELSE
			m.IdProducto=sas_solicitud.idproductoFinal
			m.Descripcion=ThisForm.Pageframe1.Page3.Producto.Value
		
		ENDIF
		
		
		m.Contador=0
		SCAN
		*DELETE FROM os_movimiento_detalle WHERE talle='xx'		
		*	DELETE
			IF talle="xx"
				replace IdProducto WITH  m.IDProducto,;
				Descripcion WITH m.Descripcion
			ENDIF 	
		m.Contador = m.Contador + 1 
		ENDSCAN
		
		
		
		IF m.Contador=0 AND sas_solicitud.Estado="D"
			ThisForm.Pageframe1.Page4.Tsgrid1.agregaritem()
			*This.parent.parent.Page4.Tsgrid1.agregaritem()
			replace talle WITH 'xx' IN os_movimiento_detalle
			ThisForm.Pageframe1.Page4.Tsgrid1.producto.Txtproducto.Tag="xx"
			ThisForm.Pageframe1.Page4.Tsgrid1.producto.Txtproducto.Value=IIF(EMPTY(NVL(sas_solicitud.idproductoFinal,'')),sas_solicitud.idproducto,sas_solicitud.idproductoFinal)
			ThisForm.Pageframe1.Page4.Tsgrid1.producto.Txtproducto.valid()
			this.writebuffer()		
		ENDIF
			 
			
		
		
		
		*VG 12-07-2012
		*Validamos que no se repita el Nro de HC
		
		IF NOT EMPTY(NVL(sas_solicitud.nrosolicitud,''))
			TEXT TO cmdSQL noshow
				SELECT ISNULL(FechaAgenda,FechaLlamada) as Fecha FROM sas_Servicios WHERE IdEmpresa=?oApp.Empresa 
				and nrosolicitud=?sas_solicitud.nrosolicitud and IdSolicitud<>?sas_solicitud.IdSolicitud
		
			ENDTEXT
			sql(cmdSQL,'_cRepetido')
			IF RECCOUNT('_cRepetido')>0
				MESSAGEBOX("El Nro de Servicio Indicado ya existe en fecha " + DTOC(_cRepetido.Fecha),48,TASTRADE_LOC)
				RETURN .f. 
			ENDIF
		ENDIF
		
			
		
		
		
		
		*VG Recuperamos la cantidad de servicios realizados a este paciente en el mes
		TEXT TO cmdSQL noshow
			Select COUNT(*) Cantidad from Sas_solicitudServ 
			where IdEmpresa=?oApp.Empresa and IdSeguro=?sas_solicitud.idseguro and NroSeguroSocial = ?sas_solicitud.nroseguro
			and MONTH(fechaAgenda)=<<MONTH(DATE())>> and YEAR(fechaAgenda)=<<YEAR(DATE()))>>
		ENDTEXT
		
			cmdSQL = "Select COUNT(*) Cantidad from Sas_solicitudServ where IdEmpresa=?oApp.Empresa and IdSeguro=?sas_solicitud.idseguro and NroSeguro = ?sas_solicitud.nroseguro " +;
			" and IdSolicitud<> ?sas_solicitud.IdSolicitud "
		*	thisform.runsql(cmdSQL,'cCantidad')
										
		TEXT TO CMDSQL NOSHOW
			SELECT cs.precio,s.Cantidad
			FROM sas_costoservicio cs inner join sas_Seguro s on cs.IdSeguro = s.IdSeguro
			WHERE cs.idseguro = ?sas_solicitud.idseguro and cs.idtiposervicio = ?sas_solicitud.idtiposervicio and cs.idempresa = ?oApp.Empresa
		ENDTEXT
		
		*sql (cmdsql, "ccs")
		
		IF EMPTY(sas_solicitud.IdSeguro)
		*	replace sas_solicitud.IdSeguro WITH null IN sas_solicitud
		*	replace sas_solicitud.importe WITH null IN sas_solicitud
		ELSE 
			
			*VG Vemos si la cantidad todavia no supero el limite fijado, si supera se cobra, si no, no.
		*!*		IF cCantidad.Cantidad>ccs.Cantidad 
		*!*			replace sas_solicitud.importe WITH ccs.precio IN sas_solicitud
		*!*			*thisFORM.pageframe1.page3.txtImporte.Value = ccs.precio
		*!*		ELSE
		*!*			replace sas_solicitud.importe WITH 0,Comentario WITH "Exonerado" IN sas_solicitud
		*!*			*thisFORM.pageframe1.page3.txtImporte.Value = ccs.precio
		*!*			
		*!*		ENDIF
				
		ENDIF
		
		IF EMPTY(NVL(sas_solicitud.NroSalida,'')) AND NOT EMPTY(NVL(sas_solicitud.idmovil,''))
			
		* VG 25-07-2012
		* Por el momento suprimimos esta funcionalidad
		* hasta una mejor implementacion
		
		
		*!*			TEXT TO cmdSQL noshow
		*!*				SELECT TOP 1 substring(nroSalida,charindex('/',NroSalida)+1,10) as Nro
		*!*				from sas_servicios
		*!*				where IdEmpresa = ?oApp.Empresa
		*!*				and IdMovil = ?sas_solicitud.IdMovil
		*!*				order by convert(int,substring(nroSalida,charindex('/',NroSalida)+1,10)) desc
		*!*			ENDTEXT
		*!*			=sql(cmdSQL,'cNro')
		
		
		*!*			IF RECCOUNT('cNro')= 0
		*!*				mNro = 1
		*!*			ELSE
		*!*				mNro = VAL(NVL(cNro.Nro,'')) + 1
		*!*			ENDIF
		
		*!*			mNro = RTRIM(sas_solicitud.IdMovil) + '/' + ALLTRIM(STR(mNro))
		
		*!*			replace NroSalida WITH mNro IN sas_solicitud
		
		ENDIF
		
		
		
		IF EMPTY(sas_solicitud.IdPaciente)
			replace IdPaciente WITH null IN sas_solicitud
		ENDIF
			
		IF EMPTY(NVL(sas_solicitud.idmovil,''))
			replace idmovil WITH null IN sas_solicitud
		ELSE
			replace Estado WITH "D" IN sas_Solicitud
		ENDIF
			
		
		
		IF EMPTY(sas_Solicitud.FechaAgenda)
			replace FechaAgenda WITH getdate() IN sas_Solicitud
		ENDIF
		
		
		IF EMPTY(sas_Solicitud.chofer)
			replace chofer WITH null IN sas_Solicitud
		ENDIF
		
		IF EMPTY(sas_Solicitud.paramedico)
			replace paramedico WITH null IN sas_Solicitud
		ENDIF
			
		IF EMPTY(sas_Solicitud.medico)
			replace medico WITH null IN sas_Solicitud
		ENDIF
		
		IF EMPTY(sas_Solicitud.IdSucursal)
			replace IdSucursal WITH null IN sas_Solicitud
		ENDIF
		
		
		*VG 17-09-2012
		*Se generar en el NEW
		
		
		*!*	IF EMPTY(NVL(sas_Solicitud.IdRemision,0))
		*!*		replace IdRemision WITH newid('os_remision') IN sas_Solicitud
		*!*	ENDIF
			
	ENDPROC

	PROCEDURE verificarsaldo
		LPARAMETERS lcNroContrato
		
		TEXT TO cmdSQL NOSHOW TEXTMERGE 
			SELECT     f.IdComprobante + '-' + CONVERT(char(10), f.Numero) AS Factura,s.Vencimiento, s.Saldo
			FROM         vt_factura AS f INNER JOIN
			                      vt_forma_pago AS s ON f.IdFactura = s.IdFactura
			WHERE     (s.Saldo > 0) AND (s.Vencimiento < GETDATE())
						and f.IdEmpresa=?oApp.Empresa and f.NroContrato='<<lcNroContrato>>'
		ENDTEXT
		
		ThisForm.Pageframe1.Page1.grdSaldo.RecordSource=''
		this.runsql(cmdSQL,'cSaldos')
		ThisForm.Pageframe1.Page1.grdSaldo.RecordSource='cSaldos'
		IF RECCOUNT('cSaldos')>0
		*	MESSAGEBOX("Este contrato posee facturas pendientes de pago.",48,TASTRADE_LOC)
		ENDIF
		
		
		
	ENDPROC

