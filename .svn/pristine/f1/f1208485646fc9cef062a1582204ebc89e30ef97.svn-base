				IF LEFT(ThisForm.Pageframe2.Page1.txtCodigo.Value,1)="-"		
					ThisForm.Pageframe2.Page1.txtCodigo.Value = SUBSTR(ThisForm.Pageframe2.Page1.txtCodigo.Value,2,LEN(ThisForm.Pageframe2.Page1.txtCodigo.Value))
