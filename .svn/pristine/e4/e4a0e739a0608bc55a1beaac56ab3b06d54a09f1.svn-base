	PROCEDURE afteraddnew
			TEXT TO cmdSQL noshow
				SELECT MAX(LEFT(nrocontrato,5)) Nro FROM sas_Contrato
				where IdEmpresa = ?oApp.Empresa
				and isnumeric(LEFT(nrocontrato,5))=1
			ENDTEXT
			
			this.runsql(cmdSQL,'cNro')
			
			IF RECCOUNT('cNro')>0
				replace NroContrato WITH RIGHT('00000'+ ALLTRIM(STR(VAL(cNro.Nro)+1)),5) IN sas_Contrato			
			ELSE
				replace NroContrato WITH '00001' IN sas_Contrato			
			ENDIF
		
	ENDPROC

	PROCEDURE Refresh
		ThisForm.Pageframe1.Page1.Pageframe1.Page1.Refresh()
		ThisForm.Pageframe1.Page1.Pageframe1.Page2.Refresh()
		ThisForm.Pageframe1.Page1.Pageframe1.Page3.Refresh()
		ThisForm.Pageframe1.Page1.Pageframe1.Page4.Refresh()
		ThisForm.Pageframe1.Page1.Pageframe1.Page5.Refresh()
		
	ENDPROC

	PROCEDURE validar
		IF RECCOUNT('sas_CostoAfiliados')>0
			SELECT sas_Afiliados
			COUNT FOR IdEstado='A' TO m.Cantidad1
			SELECT sas_AfiliadosIn
			COUNT FOR IdEstado='A' TO m.Cantidad2
			m.Cantidad=m.Cantidad1 + m.Cantidad2
			
			SELECT TOP 1 Precio FROM sas_CostoAfiliados WHERE NVL(Cantidad,0) <= m.Cantidad ORDER BY Cantidad DESC INTO CURSOR _cCosto
			
			replace Importe WITH _cCosto.Precio*Cantidad  IN sas_Contrato
		ENDIF
			
			 	   
	ENDPROC

	PROCEDURE Pageframe1.Page1.Pageframe1.Page1.Activate
		this.Refresh()
		
	ENDPROC

	PROCEDURE Pageframe1.Page1.Pageframe1.Page2.Activate
		this.Refresh()
		
	ENDPROC

