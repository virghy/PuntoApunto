	PROCEDURE refreshform
		Parameter _IDENTIDAD
		Local LCOLDID, LCNEWID, LCESTADO
		If  .Not. Thisform.EDITMODE .And.  .Not. This.ESNUEVO
			LCOLDID = Thisform.TABLA1
			LCOLDID = LCOLDID+'.'+Thisform.IDENTIDAD
			LCOLDID = &LCOLDID
			LCESTADO = 0
			If Pcount()>0
				LCNEWID = '_Identidad'
