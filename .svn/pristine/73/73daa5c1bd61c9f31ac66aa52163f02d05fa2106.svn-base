				ENDIF
				SQLMORERESULTS(lnHndConn)
				SQLCANCEL(lnHndConn)
				SQLDISCONNECT(lnHndConn)
			ENDIF
			
		ELSE
			lcSQL = lcSQL + ' into cursor xClave'
			&lcSQL
		ENDIF
		
			
		
		
		if not empty(oldalias)
			select &OldAlias
		endif
		
		if RECCOUNT('xClave')>0
		
