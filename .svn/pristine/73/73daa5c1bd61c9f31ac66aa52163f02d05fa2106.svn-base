	PROCEDURE asktosave		&& Pide al usuario que guarde los cambios si es necesario.
		LOCAL LNANSWER
		LNANSWER = MESSAGEBOX("¿Desea guardar primero sus cambios?", 035, "Futura Software")
		DO CASE
		CASE LNANSWER=6
		IF  .NOT. THISFORM.SAVE()
		LNANSWER = 2
		ENDIF
		CASE LNANSWER=7
		THISFORM.RESTORE()
		ENDCASE
		RETURN LNANSWER
	ENDPROC

