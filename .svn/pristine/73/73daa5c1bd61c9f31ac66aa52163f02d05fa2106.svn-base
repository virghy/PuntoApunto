			RETURN 3
		ENDIF
		LCTABLA = THIS.AREAPRINCIPAL
		SELECT &LCTABLA
		IF  .NOT. EOF()
			IF THISFORM.ISNEWANDEMPTY()
				THISFORM.RESTORE()
			ELSE
