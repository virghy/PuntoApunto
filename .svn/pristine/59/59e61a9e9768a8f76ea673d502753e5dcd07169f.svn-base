	PROCEDURE validar
		IF EMPTY(vcheques.nrocheque)
			MESSAGEBOX("Ingrese el Nro de Cheque",64,TASTRADE_LOC)
			RETURN .F.
		ENDIF
			
		
		If Oldval('NroOrden', 'vCheques')<>0 .And. VCHEQUES.NROORDEN<>Oldval('NroOrden', 'vCheques')
			MENSAJE = 'Al cambiar la Orden de Pago, se va a revertir el Pago para la Orden Nº:'+Alltrim(Str(Oldval('NroOrden', 'vCheques')))+Chr(13)+'Desea continuar?'
			If Messagebox(MENSAJE, 036, "Futura Software")=6
				Return .T.
			Else
				Return .F.
			Endif
		ENDIF
		
		Replace TOTALCHEQUE With Thisform.PAGEFRAME1.PAGE1.TXTIMPORTE.Value In VCHEQUES
		If Empty(Thisform.PAGEFRAME1.PAGE1.TXTFECHAPAGO.Value)
			Replace FECHADIFERIDA With .Null. In VCHEQUES
		Endif
		If  .Not. Isnull(VCHEQUES.FECHADIFERIDA)
			This.idasiento='02'
		Else
			This.idasiento='13'
		Endif
		
		IF 	This.DESDEORDENPAGO AND !EMPTY(this.idfactura)
			this.runsql("Update cp_Factura set NroCheque=?vcheques.nrocheque,IdDeposito=?vcheques.IdDeposito where IdFactura =?thisform.idfactura",'xx',this.hndconn)
		ENDIF
			
		IF  oApp.Asentar AND .NOT. EMPTY(THIS.IDASIENTO) .AND.  .NOT. EMPTY(EVALUATE(THIS.ASIENTORECORDSOURCE))
				IF !THISFORM.PAGEFRAME1.PAGES(THISFORM.PAGEFRAME1.PAGECOUNT-1).ASIENTO.Validar()
					RETURN .f.
				ENDIF
					
				IF  .NOT. EMPTY(VCHEQUES.FECHADIFERIDA) .AND.  .NOT. ISNULL(VCHEQUES.FECHADIFERIDA)
					RETURN THISFORM.PAGEFRAME1.PAGES(THISFORM.PAGEFRAME1.PAGECOUNT).ASIENTO.Validar()
				ENDIF
			
		ENDIF
			
	ENDPROC

