	PROCEDURE formatocheque
		Parameter LIDMONEDA, LIDBANCO, LFECHADIFERIDA
		If LFECHADIFERIDA
			LTIPO = 'D'
		Else
			LTIPO = 'N'
		Endif
		INFORME = 'TS_FORMATOCHEQUE.frx'
		This.RUNSQL('Select * from ts_FormatoCheque where IdMoneda=?m.lIdMoneda and IdBanco = ?m.lIdBanco and Tipo=?m.lTipo', 'cFormato')
		Select * From (INFORME) Into Table lcCheque.frx
		Select lcCheque
		Goto Top
		Replace HPOS With (this.MargenIzquierdo +  Val(Substr(CFORMATO.IMPORTE, 1, At(',', CFORMATO.IMPORTE)-1)))*393.7 , VPOS With (this.margensuperior + Val(Substr(CFORMATO.IMPORTE, At(',', CFORMATO.IMPORTE)+1)))*393.7  All For UNIQUEID='_IMPORTE' In lcCheque
		Goto Top
		Replace HPOS With (this.MargenIzquierdo  +  Val(Substr(CFORMATO.IMPORTELETRAS, 1, At(',', CFORMATO.IMPORTELETRAS)-1)))*393.7 , VPOS With (this.margensuperior +Val(Substr(CFORMATO.IMPORTELETRAS, At(',', CFORMATO.IMPORTELETRAS)+1)))*393.7  All For UNIQUEID='_IMPORTEL' In lcCheque
		Goto Top
		Replace HPOS With (this.MargenIzquierdo  +  Val(Substr(CFORMATO.IMPORTELETRAS1, 1, At(',', CFORMATO.IMPORTELETRAS1)-1)))*393.7 , VPOS With (this.margensuperior +Val(Substr(CFORMATO.IMPORTELETRAS1, At(',', CFORMATO.IMPORTELETRAS1)+1)))*393.7  All For UNIQUEID='_IMPORTEL1' In lcCheque
		Goto Top
		Replace HPOS With (this.MargenIzquierdo  +  Val(Substr(CFORMATO.BENEFICIARIO, 1, At(',', CFORMATO.BENEFICIARIO)-1)))*393.7 , VPOS With (this.margensuperior +Val(Substr(CFORMATO.BENEFICIARIO, At(',', CFORMATO.BENEFICIARIO)+1)))*393.7  All For UNIQUEID='_BENEF' In lcCheque
		Goto Top
		Replace HPOS With (this.MargenIzquierdo  +  Val(Substr(CFORMATO.DIA, 1, At(',', CFORMATO.DIA)-1)))*393.7 , VPOS With (this.margensuperior + Val(Substr(CFORMATO.DIA, At(',', CFORMATO.DIA)+1)))*393.7  All For UNIQUEID='_DIA' In lcCheque
		Goto Top
		Replace HPOS With (this.MargenIzquierdo  +  Val(Substr(CFORMATO.MES, 1, At(',', CFORMATO.MES)-1)))*393.7 , VPOS With (this.margensuperior +Val(Substr(CFORMATO.MES, At(',', CFORMATO.MES)+1)))*393.7  All For UNIQUEID='_MES' In lcCheque
		Goto Top
		Replace HPOS With (this.MargenIzquierdo  +  Val(Substr(CFORMATO.AÑO, 1, At(',', CFORMATO.AÑO)-1)))*393.7 , VPOS With (this.margensuperior +Val(Substr(CFORMATO.AÑO, At(',', CFORMATO.AÑO)+1)))*393.7  All For UNIQUEID='_AÑO' In lcCheque
		Goto Top
		Replace HPOS With (this.MargenIzquierdo  +  Val(Substr(CFORMATO.DIAPAGO, 1, At(',', CFORMATO.DIAPAGO)-1)))*393.7 , VPOS With (this.margensuperior +Val(Substr(CFORMATO.DIAPAGO, At(',', CFORMATO.DIAPAGO)+1)))*393.7  All For UNIQUEID='_DIAPAGO' In lcCheque
		Goto Top
		Replace HPOS With (this.MargenIzquierdo  +  Val(Substr(CFORMATO.MESPAGO, 1, At(',', CFORMATO.MESPAGO)-1)))*393.7 , VPOS With (this.margensuperior +Val(Substr(CFORMATO.MESPAGO, At(',', CFORMATO.MESPAGO)+1)))*393.7  All For UNIQUEID='_MESPAGO' In lcCheque
		Goto Top
		Replace HPOS With (this.MargenIzquierdo  +  Val(Substr(CFORMATO.AÑOPAGO, 1, At(',', CFORMATO.AÑOPAGO)-1)))*393.7 , VPOS With (this.margensuperior +Val(Substr(CFORMATO.AÑOPAGO, At(',', CFORMATO.AÑOPAGO)+1)))*393.7  All For UNIQUEID='_AÑOPAGO' In lcCheque
		Use In lcCheque
		Use In (INFORME)
		Return 'lcCheque.frx'
	ENDPROC

	PROCEDURE generardesdefactura
		TEXT TO CMDSQL
			SELECT     f.Fecha, f.IdProveedor,f.FacturaProveedor, p.Razon, f.Referencia,
					       Importe = isnull(Gravada,0)+isnull(Exenta,0)+ isnull(Iva,0)
			FROM         cp_factura AS f INNER JOIN
		                      cp_proveedor AS p ON f.IdEmpresa = p.IdEmpresa AND f.IdProveedor = p.IdProveedor
		    where f.IdFactura = ?thisform.idfactura
				 	and f.IdEmpresa = ?oApp.Empresa
				   and idDeposito is null                   
		ENDTEXT
		If Thisform.RUNSQL(CMDSQL, 'ts_cORdenPAgo')>0 .And. Reccount('ts_cORdenPAgo')>0
			Replace IMPORTE With TS_CORDENPAGO.IMPORTE, DEPOSITANTE With TS_CORDENPAGO.Razon, BENEFICIARIO With TS_CORDENPAGO.Razon, REFERENCIA With 'Factura Nº '+Alltrim(TS_CORDENPAGO.FacturaProveedor)+' '+NVL(TS_CORDENPAGO.Referencia,''), FECHA With TS_CORDENPAGO.Fecha In VCHEQUES
		*!*			IF !EMPTY(NVL(TS_CORDENPAGO.IDCUENTA,""))
		*!*				Replace IDCUENTA With TS_CORDENPAGO.IDCUENTA In VCHEQUES
		*!*				Thisform.PAGEFRAME1.PAGE1.CUENTA.InteractiveChange()
		*!*			ENDIF
			Thisform.REFRESHFORM()
		ELSE
			Messagebox('No se encuentra la factura o ya ha sido cancelada.', 64, "Futura Software")	
		ENDIF
		
	ENDPROC

	PROCEDURE generarorden
		m.NROORDEN = Thisform.PAGEFRAME1.PAGE1.NROORDEN.Value
		*!*			SELECT a.nroorden,
		*!*			       a.nombre,
		*!*			       a.beneficiario,
		*!*			       a.idmoneda,
		*!*			       a.Detalle,
		*!*			       Importe =
		*!*			       isnull(case
		*!*			          when a.IdMoneda <> b.IdMoneda then sum( b.importe * b.cotizacion )
		*!*			          when a.IdMoneda = b.IdMoneda then sum( b.importe )
		*!*			       end,a.Importe),
		*!*			      convert(char(4),a.IdCuenta) as IdCuenta
		*!*			  FROM cp_ordenpago a left JOIN cp_orddet_pago b ON a.IdOrdenPago = b.nroorden
		*!*			 WHERE a.nroorden = ?m.NroOrden
		*!*			 	and a.IdEmpresa = ?oApp.Empresa
		*!*			   AND a.autorizado = 1
		*!*			   and idDeposito is null 
		*!*			GROUP BY a.nroorden,a.Detalle,a.Importe, a.nombre, a.beneficiario, a.idmoneda, b.idmoneda, a.IdCuenta
		
		
		TEXT TO CMDSQL
				SELECT a.nroorden,
				       a.nombre,
				       a.beneficiario,
				       a.idmoneda,
				       a.Detalle,
				       Importe = ISNULL((Select SUM(Valorizado) from cp_orddet_pago b where a.IdOrdenPago = b.nroorden),a.Importe),
				      convert(char(4),a.IdCuenta) as IdCuenta
				  FROM cp_ordenpago a 
				 WHERE a.nroorden = ?m.NroOrden
				 	and a.IdEmpresa = ?oApp.Empresa
				   AND a.autorizado = 1
				   and idDeposito is null 
		ENDTEXT
		If Thisform.RUNSQL(CMDSQL, 'ts_cORdenPAgo')>0 .And. Reccount('ts_cORdenPAgo')>0
			Replace IMPORTE With TS_CORDENPAGO.IMPORTE, DEPOSITANTE With TS_CORDENPAGO.NOMBRE, BENEFICIARIO With TS_CORDENPAGO.BENEFICIARIO, REFERENCIA With 'OP Nº '+Alltrim(Str(M.NROORDEN))+' '+NVL(TS_CORDENPAGO.DETALLE,''), FECHA With Date() In VCHEQUES
				IF !EMPTY(NVL(TS_CORDENPAGO.IDCUENTA,""))
					Replace IDCUENTA With TS_CORDENPAGO.IDCUENTA In VCHEQUES
					Thisform.PAGEFRAME1.PAGE1.CUENTA.InteractiveChange()
				ENDIF
			Thisform.REFRESHFORM()
		Else
			Messagebox('No se encuentra la Orden de Pago o ya ha sido cancelada.', 64, "Futura Software")
		*	This.Value = Val(This.Tag)
			Thisform.PAGEFRAME1.PAGE1.NROORDEN.Value=VAL(Thisform.PAGEFRAME1.PAGE1.NROORDEN.tag)
		Endif
	ENDPROC

	PROCEDURE imprimir
		Local LINFORME
		TEXT TO CMDSQL NOSHOW
			select Decimales,a.fecha,a.fechadiferida,a.idcuenta,a.totalcheque monto,a.beneficiario as Depositante, c.idmoneda,c.idtipo,c.formato,c.IdBanco
			from ts_depositos_base a inner join  ts_detdepos_base b on a.iddeposito=b.iddeposito
			left join ts_cuentas c on a.idcuenta=c.idcuenta and c.idempresa=?oApp.Empresa
			left join bs_monedas m on c.IdMoneda = m.IdMoneda
			where a.iddeposito=?vcheques.iddeposito
		ENDTEXT
		
		This.RUNSQL(CMDSQL, 'rtraercheque')
		Select RTRAERCHEQUE
		
		LCREPORT = LEERINI('Cheques', 'OPCIONES')
		If Empty(LCREPORT)
			LCREPORT = 'TS_C'
			= ESCRIBIRINI(LCREPORT, 'Cheques', 'OPCIONES')
		Endif
		
		LCDESTINO = LEERINI('Destino Impresion', 'OPCIONES')
		If Empty(LCDESTINO)
			LCDESTINO = 'PREVIEW NOCONSOLE'
			= ESCRIBIRINI(LCDESTINO, 'Destino Impresion', 'OPCIONES')
		Endif
		
		
		
		* Para permitir al usuario cortar las palabras
		
		mLetras= ALLTRIM(numeral(INT(monto)))
		IF Decimales > 0
			mLetras = mLetras + ' CON ' + SUBSTR(STR(Monto - INT(monto),3,2),2) + '/100'
		ENDIF
		
		mletras= mletras + REPLICATE('-',130)
		mletras1 = LEFT(mletras,57)
		mletras2 = substr(mletras,58,65)
		
		*TS_PERS_CHEQUE
		IF oApp.LeerConstante("TS_PERS_CHEQUE","Char(1)")="S"                                
			CREATE CURSOR Montos(Importe c(200),Importe1 c(200),importe2 c(200))
			INSERT INTO Montos(Importe,Importe1,Importe2) VALUES (mletras,mletras1,mletras2)
		
			DO FORM ts_ajusteImpresion 
			mletras1 = montos.Importe1
			mletras2 = montos.Importe2
		ENDIF
		
		Select RTRAERCHEQUE
		
		LINFORME = This.FORMATOCHEQUE(Nvl(RTRAERCHEQUE.IDMONEDA, ''), Nvl(RTRAERCHEQUE.IDBANCO, ''),  .Not. Empty(Nvl(RTRAERCHEQUE.FECHADIFERIDA, {})))
		Report Form &LINFORME  &LCDESTINO
		RETURN
		
		
		
		LINFORME = LCREPORT+Alltrim(Nvl(RTRAERCHEQUE.IDMONEDA, ''))+Alltrim(Nvl(RTRAERCHEQUE.IDBANCO, ''))
		If  .Not. Empty(Nvl(RTRAERCHEQUE.FECHADIFERIDA, ''))
			LINFORME = LINFORME+'D'
		Endif
		LINFORME = Alltrim(LINFORME)+'.FRX'
		If  .Not. File(LINFORME)
			Messagebox('El Archivo de formato '+Alltrim(LINFORME)+' no se ha encontrado.', "Futura Software")
			Return .F.
		Endif
		LINFORME = SETINFORME(LINFORME)
		Report Form &LINFORME  &LCDESTINO
	ENDPROC

	PROCEDURE Init
		Parameter LNNROORDEN, LNNROCUENTA, lcOrigen
		*21-09-07 
		* Se agregó el parametro lcOrigen 
		* para controlar cuando se paga 
		* directamente una factura
		
		
		DoDefault()
		Thisform.PAGEFRAME1.PAGE1.MONEDA.ControlSource = "cCuentas.IdMoneda"
		Thisform.PAGEFRAME1.PAGE1.CUENTA.ListIndex = 1
		If Pcount()>0
			This.ADDNEW()
			IF VARTYPE(lcOrigen)='C'	&& Origen Factura
			this.idfactura=LNNROORDEN
				this.generardesdefactura()
			ELSE && Origen Orden de Pago
				Thisform.PAGEFRAME1.PAGE1.NROORDEN.Value = LNNROORDEN
				This.GENERARORDEN()
			ENDIF
			If Pcount()>1
				Thisform.PAGEFRAME1.PAGE1.CUENTA.Value = LNNROCUENTA
			ENDIF
			
			This.DESDEORDENPAGO = .T.
			this.Refresh()
		ENDIF
		
		IF THIS.RUNSQL("Select convert(int,dbo.LeerConstante(?oApp.Empresa,'TS_MARGEN_SUP')) as Superior,convert(int,dbo.LeerConstante(?oApp.Empresa,'TS_MARGEN_IZQ')) as Izquierdo", 'cConfig')>0
			this.margensuperior=cConfig.Superior
			this.margenizquierdo=cConfig.Izquierdo
		
		ENDIF
		
		
		
	ENDPROC

