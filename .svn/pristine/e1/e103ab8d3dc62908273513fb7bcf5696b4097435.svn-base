		lcFile=GETPICT("","Seleccione el archivo de imagen")
		SELECT st_producto
		IF !EMPTY(lcFile)
		*	MESSAGEBOX(lcFile)
			APPEND MEMO Imagen FROM (lcFile) OVERWRITE  
			This.parent.Image1.PictureVal=st_producto.Imagen
			
		ENDIF
		
		
		
	ENDPROC

	PROCEDURE PAGEFRAME1.PAGE1.Pageframe2.Page5.Tscommandbutton2.Click
		SELECT st_producto
		replace Imagen WITH null 
		This.parent.Image1.PictureVal=""
			
		
		
	ENDPROC

	PROCEDURE PAGEFRAME1.PAGE1.Pageframe2.Page6.Tsgrid1.Lista.listaPrecio.InteractiveChange
		REPLACE VT_PRECIOS.MONEDA WITH CLISTA.MONEDA
	ENDPROC

	PROCEDURE PAGEFRAME1.PAGE1.Pageframe2.Page7.Activate
		this.Refresh()
	ENDPROC

	PROCEDURE PAGEFRAME1.PAGE1.Tscommandbutton1.Click
		IF MESSAGEBOX("Esta seguro que desea recalcular el costo de este producto?" + CHR(13) + "Esto puede demorar unos minutos.",32+4,TASTRADE_LOC)=6
			thisform.runsql("Exec RecalcularCostos ?oApp.Empresa,?IdProducto")
			MESSAGEBOX("Proceso finalizado." + CHR(13) + "Vuelva a consultar el producto para ver los datos actualizados.",64,TASTRADE_LOC)
		ENDIF
			
			
	ENDPROC

	PROCEDURE PAGEFRAME1.PAGE1.txtCostoProm.Refresh
		this.Value=this.Parent.costo_promedio.Value + (this.Parent.costo_promedio.Value * vt_Iva.Valor /100)
		this.Parent.txtUltCosto.Value=this.Parent.ultimo_costo.Value + (this.Parent.ultimo_costo.Value* vt_Iva.Valor /100)
		this.Parent.txtCostoRepo.Value=this.Parent.txtCosto_repo.Value + (this.Parent.txtCosto_repo.Value* vt_Iva.Valor /100)
		
		
		
	ENDPROC

ENDDEFINE
