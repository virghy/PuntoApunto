		THIS.REFRESH()
	ENDPROC

	PROCEDURE Refresh
		DODEFAULT()
		IF !thisform.editmode
			ThisForm.PAGEFRAME1.PAGE1.Pageframe2.Page2.Tsgrid1.Refresh()
			ThisForm.PAGEFRAME1.PAGE1.Pageframe2.Page1.Tsgrid1.Refresh()
			ThisForm.PAGEFRAME1.PAGE1.Pageframe2.Page3.Tsgrid2.Refresh()
			ThisForm.PAGEFRAME1.PAGE1.Pageframe2.Page4.Tsgrid1.Refresh()
			ThisForm.PAGEFRAME1.PAGE1.Pageframe2.Page6.Tsgrid1.Refresh()
			ThisForm.PAGEFRAME1.PAGE1.Pageframe2.Page7.grdSt_valorenergetico.Refresh()
		ENDIF
		
	ENDPROC

	PROCEDURE validar
		IF EMPTY(ST_PRODUCTO.PROVEEDOR)
		REPLACE PROVEEDOR WITH .NULL. IN ST_PRODUCTO
		ENDIF
		
		IF EMPTY(st_producto.grupo1)
			replace grupo1 WITH .null. IN st_producto
		ENDIF
			
			
		IF NVL(st_producto.filtrasucursal,.f.) AND RECCOUNT('st_productosucursal')=0
			MESSAGEBOX("Debe indicar al menos una sucursal si desea filtrar por sucursal.",64,TASTRADE_LOC)
			RETURN .F.
		ENDIF
			
	ENDPROC

