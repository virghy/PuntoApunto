	PROCEDURE PAGEFRAME1.PAGE1.Pageframe2.Page2.Tscommandbutton1.Click
		*!*	SELECT IdAtributo FROM cAtributo INTO ARRAY aAtributo
		
		*!*	SELECT distinct Valor,IdAtributo FROM st_ProductoAtributo WITH (BUFFERING=.T.) INTO  ARRAY  aValores
		*!*	*SELECT cValores
		*!*	*COPY TO ARRAY aValores
		*!*	*ACOPY(aValores,AValores2)
		*!*	SET STEP ON
		
		*!*	FOR i=1 TO 1 &&ALEN(aValores,1)
		*!*		oAtributo = aValores(i,2)
		*!*		oValor = aValores(i,1)
		*!*		cNuevo = aValores(i,1)
		*!*		SELECT distinct Valor,IdAtributo FROM st_ProductoAtributo WITH (BUFFERING=.T.) WHERE IdAtributo <> oAtributo INTO  ARRAY  aValores2 	
		*!*		FOR x=1 TO ALEN(aValores2,1)
		*!*			IF oAtributo <> aValores2(x,2) AND oValor <> aValores2(x,1) 			
		*!*				cNuevo = cNuevo + ' ' + aValores2(x,1)
		*!*			ENDIF 	
		*!*		ENDFOR
		*!*		? cNuevo
		*!*	ENDFOR
		LOCAL cValores,cNombre
		IF thisform.runsql("exec dbo.st_GenerarAtributo ?st_Producto.IdProducto,?st_Producto.IdEmpresa","cAtr")>0
			SELECT cAtr
			SCAN
				cValores=""
				FOR x=1 TO FCOUNT()
					cNombre=FIELD(x)
					cValores= cValores + ' ' + ALLTRIM(NVL(eval(cNombre),''))
					
				ENDFOR
				INSERT INTO st_ProductoHijo(IdEmpresa,IdProducto,Descripcion,IdPadre,iva,idtipo) ;
				VALUES (st_Producto.IdEmpresa,ALLTRIM(st_Producto.IdProducto) + TRANSFORM(RECNO('cAtr'),"@L 99"),ALLTRIM(st_Producto.Descripcion) + ' ' + cValores,st_Producto.IdProducto,st_producto.iva,;
				'P')
			ENDSCAN
			
		ENDIF
		
			This.parent.parent.Page4.Tsgrid1.Refresh()
			This.parent.parent.Page4.Activate()
			
			
			
		
	ENDPROC

