	PROCEDURE addnew
		DODEFAULT()
		THIS.PAGEFRAME1.PAGE1.TIPO.LISTINDEX = 1
		THIS.PAGEFRAME1.PAGE1.UNIDAD.LISTINDEX = 3
		THIS.PAGEFRAME1.PAGE1.IVA.LISTINDEX = 1
		THIS.PAGEFRAME1.PAGE1.MONEDA.LISTINDEX = 2
	ENDPROC

	PROCEDURE Init
		If This.RUNSQL("Select convert(int,dbo.LEERCONSTANTE(?oApp.Empresa,'ST_CODIGO_PRODUCTO')) as Valor", 'cConfig')>0
			If CCONFIG.VALOR>0
				This.CAMPOTAM = CCONFIG.VALOR
			Endif
		Endif
		DoDefault()
		
		&&VG 11/07/2009
		&& Permiso para ver los costos de productos
		DIMENSION PERMISO(6)
		PERMISO = OAPP.PERMISOS(466)
		IF .NOT. PERMISO(1)
			ThisForm.PAGEFRAME1.PAGE1.costo_promedio.Visible=.f.
			ThisForm.PAGEFRAME1.PAGE1.txtCosto_repo.Visible=.f.
			ThisForm.PAGEFRAME1.PAGE1.ultimo_costo.Visible=.f.
		ENDIF
		
		
	ENDPROC

