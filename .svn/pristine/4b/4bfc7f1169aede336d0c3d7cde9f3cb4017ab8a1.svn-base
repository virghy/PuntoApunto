		where IdEmpresa = ?oApp.Empresa
		
		ENDTEXT
		
		thisform.runsql(cmdsql,'c')
		
		WAIT windows NOWAIT "Procesando Ejercicios" 
		TEXT TO cmdSQL noshow
		INSERT INTO [ejercicios]
		           ([idempresa]
		           ,[ejercicio]
		           ,[inicioejercicio]
		           ,[finalejercicio]
		           ,[cerrado]
		           ,[fechacierre]
		           ,[activo]
