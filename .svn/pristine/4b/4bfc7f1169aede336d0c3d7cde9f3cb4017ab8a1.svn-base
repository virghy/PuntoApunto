		           ,[Notas]
		           ,[Activo]
		           ,[idcartera]
		from cp_proveedor
		where IdEmpresa = ?oApp.Empresa
		
		ENDTEXT
		
		thisform.runsql(cmdsql,'c')
		
		WAIT windows NOWAIT "Procesando 7.	Clientes" 
		TEXT TO cmdSQL noshow
		INSERT INTO [vt_clientes]
		           ([IdEmpresa]
		           ,[IdCliente]
		           ,[RazSocial]
		           ,[NomContacto]
		           ,[Cargo]
		           ,[Ruc]
		           ,[Direccion]
		           ,[Ciudad]
		           ,[Barrio]
		           ,[Telefono]
		           ,[Fax]
		           ,[Categoría]
