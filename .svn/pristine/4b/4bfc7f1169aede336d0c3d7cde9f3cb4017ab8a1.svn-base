		ENDTEXT
		
		thisform.runsql(cmdsql,'c')
		WAIT windows NOWAIT "Procesando 7a.	Comprobantes de Stock" 
		TEXT TO cmdSQL noshow
		INSERT INTO [st_cpbt_stk]
		           ([IdEmpresa]
		           ,[Cpbt_Stk]
		           ,[Descripcion]
		           ,[Titulo]
		           ,[Origen]
		           ,[Tipo]
		           ,[Act_Stk]
		           ,[Act_Valor]
		           ,[Act_Compra_Repos]
		           ,[Act_Stk_Comprometido]
		           ,[Valor_Costo]
		           ,[Ctrl_Vence]
		           ,[Imprime])
		SELECT 
		           [idempresa]=?cNuevaEmpresa
