		           ,[CargoContacto]
		           ,[Ruc]
		           ,[IdCondicion]
		           ,[Direccion]
		           ,[IdBarrio]
		           ,[IdCiudad]
		           ,[Telefono]
		           ,[Pais]
		           ,[Fax]
		           ,[Email]
