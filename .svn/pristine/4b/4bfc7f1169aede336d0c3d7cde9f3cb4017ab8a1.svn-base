		           ,[IdCategoria]
		           ,[IdCondicion]
		           ,[IdVendedor]
		           ,[IdCobrador]
		           ,[Email]
		           ,[Url]
		           ,[TipoImpuesto])
		SELECT 
		           [idempresa]=?cNuevaEmpresa
		           ,[IdCliente]
		           ,[RazSocial]
		           ,[NomContacto]
		           ,[Cargo]
		           ,[Ruc]
		           ,[Direccion]
		           ,[Ciudad]
