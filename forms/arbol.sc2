				m.Activo = OAPP.PERMISOS(IDOBJETO)
			ELSE
				m.Activo = INFORMES.DISPONIBLE
			ENDIF
			IF LCAGRUPACION='C'
				IF ALLTRIM(PARENT)='0_'
					O.ADD( , 1, ALLTRIM(KEY), ALLTRIM(TEXT), 1, 2)
