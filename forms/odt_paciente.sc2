	ENDPROC

	PROCEDURE Pageframe1.Page5.Tsgrid1.Column4.Tscommandbutton1.Click
		mImagen=GETPICT()
		IF ! empty(mImagen)
		*	APPEND MEMO Imagen  FROM (mImagen) OVERWRITE
			m.NroImagen= newID("odt_NroImagen")
			m.NombreImagen= ALLTRIM(vt_clientes.idcliente) + '-' + ALLTRIM(STR(m.NroImagen)) + "." + JUSTEXT(mImagen)
			replace ImagenPath WITH m.NombreImagen IN odt_Imagenes
			COPY FILE (mImagen) TO  thisform.pathimagen + "\"+ odt_Imagenes.ImagenPath
			This.parent.parent.parent.Image1.Picture=thisform.pathimagen + "\"+ odt_Imagenes.ImagenPath
			
		ENDIF 
			
	ENDPROC

