	PROCEDURE Pageframe1.Page4.cmdHistorico.Click
		m.dFecha=This.parent.txtdFecha.Value
		m.hFecha=This.parent.txthFecha.Value
		m.IdCliente=vt_clientes.idcliente
		TEXT TO cmdSQL noshow
		SELECT Vt_pagos.fecha, Vt_pagos.tip_reci, Vt_pagos.num_recibo, 
		Vt_pagos.idmoneda, Vt_pagos.TotalFacturas, Vt_pagos.idpago
		 FROM 
		     dbo.vt_pagos Vt_pagos
		 WHERE (  Vt_pagos.idempresa = ( ?oApp.Empresa )
		   AND  Vt_pagos.idcliente = ( ?m.Idcliente ) )
		   AND  Vt_pagos.fecha BETWEEN ?m.dFecha AND ?m.hFecha
		union 
		SELECT     f.Fecha, f.IdComprobante, f.Numero, 
		f.IdMoneda, f.TotalFactura,c.Plazo
		FROM         vt_factura AS f INNER JOIN
		                      vt_Condicion AS c ON f.IdEmpresa = c.IdEmpresa AND f.IdCondicion = c.IdCondicion
		 WHERE (  f.idempresa = ( ?oApp.Empresa )
		   AND  f.idcliente = ( ?m.Idcliente ) )
		   AND  f.fecha BETWEEN ?m.dFecha AND ?m.hFecha
		and c.Plazo=0
		ORDER BY 1,2,3
		ENDTEXT
		
		
		This.parent.grdCrm_cobros.RecordSource=SPACE(0)
		thisform.runsql(cmdsql,'Crm_cobros')
		This.parent.grdCrm_cobros.RecordSource='Crm_cobros'
		
		
		thisform.BindControls= .T.
		*REQUERY('Crm_cobros')
		REQUERY("Crm_saldoscc")
		
		***Buscamos si tuvo adelantos
		
		
		
		This.parent.grdCrm_saldoscc.Refresh()
		This.parent.grdCrm_cobros.Refresh()
		
		this.Parent.txtTotal.Refresh()
		
		thisform.runsql("SElect SUM(Saldo) Saldo,IdMoneda from vt_forma_Pago where IdEmpresa=?oApp.Empresa and IdCliente = ?m.Idcliente group by IdMoneda",'cSS')
		IF RECCOUNT('cSS')>0
			SELECT cSS
			cSaldos=''
			SCAN
				cSaldos = cSaldos + IdMoneda + ":"
				cSaldos = cSaldos + TRANSFORM(Saldo,IIF(IdMoneda='GS','999,999,999','999,999,999.99'))
				cSaldos = cSaldos + "     "
			ENDSCAN
			this.Parent.lblSaldos.Caption="Saldos " + cSaldos 
		ENDIF
		
		
		
	ENDPROC

