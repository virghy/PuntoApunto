	PROCEDURE validar
		m.IdUsuario=oApp.GetEmployeeID()
		
		SELECT odt_Tratamientos
		m.IdFactura=0
		m.Idcliente = null 
				TEXT TO cmdSQL noshow
				EXECUTE [odt_FacturaServicio] 
					   ?oApp.Empresa
					  ,?NVL(idcliente1,Idcliente)
					  ,?m.Fecha
					  ,?m.IdUsuario
					  ,?Precio
					  ,?Cantidad
					  ,?IdTratamiento
					  ,?Descripcion
					  ,?IdProfesional
					  ,?@m.IdFactura OUTPUT
					  ,?IdLista
				ENDTEXT
				
				TEXT TO cmdSQLUpd noshow
				EXECUTE [odt_FacturaServicio_Upd] 
					   ?oApp.Empresa
					  ,?NVL(idcliente1,Idcliente)
					  ,?m.Fecha
					  ,?m.IdUsuario
					  ,?Precio
					  ,?Cantidad
					  ,?IdTratamiento
					  ,?Descripcion
					  ,?IdProfesional
					  ,?m.IdFactura
					  ,?IdLista
				ENDTEXT		
		
		*SET STEP ON	
		SCAN
		
			IF INLIST(Estado,'F','T') AND EMPTY(NVL(IdProfesional,''))
				MESSAGEBOX("Debe indicar el profesional de los tratamientos finalizados",64,TASTRADE_LOC)
				RETURN .F.
			ENDIF
		
			IF INLIST(Estado,'F') AND EMPTY(NVL(FechaFin,''))
				MESSAGEBOX("Debe indicar la fecha de tratamientos finalizados",64,TASTRADE_LOC)
				RETURN .F.
			ENDIF
		
			
			IF EMPTY(FechaFin)
				replace FechaFin WITH null 
			ENDIF
		
			**Que se busca, Donde se Busca 	AT(gcFindString,gcString)
			**Verificamos si aplicamos el arancel
		
		*!*		IF AT(ALLTRIM(IdLista),thisform.arancel)>0 AND AT(ALLTRIM(IdProfesional),thisform.profesional)>0 AND EMPTY(NVL(IdFactura,0))
		*!*			replace Precio WITH 0
		*!*		ENDIF
			
		
		
			
			IF NVL(Importe,0) <>Precio*Cantidad
				replace Importe WITH Precio*Cantidad
			ENDIF
		
		
			IF EMPTY(idcliente1)
				replace idcliente1 WITH null
			ENDIF
				
				
			IF INLIST(Estado,'F','T') and EMPTY(NVL(IdFactura,0)) AND NVL(Importe,0)<>0 
				IF EMPTY(FechaFin)
					m.Fecha = Fecha
				ELSE
					m.Fecha=NVL(FechaFin,Fecha)
				ENDIF
				
				m.Idcliente = NVL(idcliente1,Idcliente)
		
				
				
				
				Rrr=this.runsql(cmdSQL,"cc",this.hndconn)
					
		*!*			IF EMPTY(m.IdFactura)
		*!*				Rrr=this.runsql(cmdSQL,"cc",this.hndconn)
		*!*			ELSE
		*!*				Rrr=this.runsql(cmdSQLUpd,"cc",this.hndconn)	
		*!*			ENDIF
				
				IF Rrr>0
					*MESSAGEBOX("Facturar Servicio")
					replace IdFactura WITH m.IdFactura
					this.runsql("Exec odt_AplicarAnticipo ?oApp.Empresa,?m.Idcliente,?m.IdFactura,?m.Fecha","cc",this.hndconn)
				ELSE
					RETURN .f.
				ENDIF
				
				 		
				
			ENDIF
		
			IF Estado='P'and NOT EMPTY(NVL(IdFactura,0))
				*MESSAGEBOX("Eliminar Factura")
				IF thisform.runsql("Delete vt_Factura where IDFactura= ?IdFactura","",this.hndconn)>0
					replace IdFactura WITH null 
				ELSE
					RETURN .F. 
				ENDIF
						
			ENDIF
			
		ENDSCAN
		
		*!*	**Aplicamos el anticipo si corresponde
		*!*	IF m.IdFactura<>0
		*!*		this.runsql("Exec odt_AplicarAnticipo ?oApp.Empresa,?m.Idcliente,?m.IdFactura,?m.Fecha","cc",this.hndconn)
		*!*	ENDIF
			
			
		
		
	ENDPROC

