	PROCEDURE Init
		DODEFAULT()
		
		m.IdUsuario=oApp.GetEmployeeID()
		thisform.runsql("Select IdVendedor,NombreVendedor from vvt_Vendedores where IDEmpresa=?oApp.Empresa and Usuario=?m.IdUsuario","vVendedor")
		this.idprofesional= vVendedor.IdVendedor
		this.nombreprof=vVendedor.NombreVendedor
		
		
		DIMENSION PERMISO(6)
		PERMISO = OAPP.PERMISOS(478)
		*SET STEP ON
		this.eliminartratam=permiso(1)
		
		IF THIS.RUNSQL("Select convert(varchar(200),dbo.LeerConstante(?oApp.Empresa,'ODT_PATH_IMAGEN')) as Path,convert(varchar(200),dbo.LeerConstante(?oApp.Empresa,'ODT_ARANCEL')) as Arancel,convert(varchar(200),dbo.LeerConstante(?oApp.Empresa,'ODT_PROFESIONAL')) as Profesional", 'cConfig')>0
			IF RECCOUNT('cConfig')>0
				THISFORM.pathimagen=ALLTRIM(cConfig.Path)	
				THISFORM.ARAncel=ALLTRIM(cConfig.Arancel)	
				THISFORM.profesional=ALLTRIM(cConfig.Profesional)	
			ENDIF
		ENDIF
		
		*!*	IF THIS.RUNSQL("Select convert(varchar(200),dbo.LeerConstante(?oApp.Empresa,'ODT_ARANCEL')) as Arancel", 'cConfig')>0
		*!*		IF RECCOUNT('cConfig')>0
		*!*			THISFORM.ARAncel=ALLTRIM(cConfig.Arancel)	
		*!*		ENDIF
		*!*	ENDIF
		
		*!*	IF THIS.RUNSQL("Select convert(varchar(200),dbo.LeerConstante(?oApp.Empresa,'ODT_PROFESIONAL')) as Profesional", 'cConfig')>0
		*!*		IF RECCOUNT('cConfig')>0
		*!*			THISFORM.profesional=ALLTRIM(cConfig.Profesional)	
		*!*		ENDIF
		*!*	ENDIF
		
	ENDPROC

